<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.it.shop.englishShoping.mapper.AddressDao">
<select id="getList" resultType="com.it.shop.englishShoping.domain.Address" parameterType="string">
  select  * from address where userid = #{uid}
</select>
  <select id="getById" resultType="com.it.shop.englishShoping.domain.Address"   parameterType="string">
      select  * from address where userid = #{uid} and id = #{id}
  </select>


</mapper>